<?xml version="1.0"?>
<robot name="myfirst">
  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="other">
    <color rgba="1 0 0.8 1"/>
  </material>
  <velocity name="vel">
    <speed spd="50"/>
  </velocity>
  

  <link name="Rail">
    <visual>
      <geometry>
        <cylinder length="4" radius=".05"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="4" radius=".05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="100"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0" iyz="0.0" izz="0"/>
    </inertial>
  </link>

  <link name="cart">
    <visual>
      <geometry>
        <box size="0.6 0.3 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.6 0.3 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="100"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0" iyz="0.0" izz="0"/>
    </inertial>
  </link>

  <joint name="rail_cart" type="prismatic">
    <axis xyz="1 0 0"/>
    <parent link="Rail"/>
    <child link="cart"/>
    <limit effort="0" lower="-2" upper="2" velocity="25"/>
    <origin rpy="0 1.570796 0" xyz="0 0 0"/>
  </joint>

